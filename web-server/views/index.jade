extends base

block menu
    ul#nav
        li: a(href='/') 首页
        li: a(href='user') 用户
        li: a(href='partition') 分服
        li: a(href='store') 商店
        li: a(href='event') 活动
        li: a(href='control') 设置
            ul
                li: a(href='import') 导入
                li: a(href='export') 导出
        li: a(href='admin') 管理员
    p.user
        if user
            | 你好 #{user.name}, <a href='logout'>登出</a>

